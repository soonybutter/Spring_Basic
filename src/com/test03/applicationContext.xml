<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id = "yang" class="com.test03.Developer">
		<constructor-arg name = "name" value="양다연"/>
		<constructor-arg name = "salary" value="2500000"/>
		<property name ="dept" value="개발"/>
	</bean>
	
	<!-- id: park class:Enginner 박준형, 3000000, 기술 -->
	<bean id="park" class="com.test03.Developer">
		<constructor-arg index="0" value="박준형"/>
		<constructor-arg index="1" value="3000000"/>
		<property name="dept" value="기술"/>
	</bean>


</beans>
