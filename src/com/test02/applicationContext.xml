<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- id:yang 양다연, 2000000 -->
	<!-- 생성자 -->
	<bean id="yang" class="com.test02.Emp">
		<constructor-arg name="name" value="양다연"/>
		<constructor-arg name="salary" value="2000000"/>
	</bean> 
	
	<!-- id: park 박준형, 2500000 -->
	<!-- setter -->
	<bean id="park" class="com.test02.Emp">
		<property name="name" value="박준형"></property>
		<property name="salary" value="2500000"></property>
	</bean>
</beans>
